<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>

  var employee = [
    {name: "Michael Scott", title: "Manager", email: "mscott@work.com",
    notes: "Manages the Scranton branch. Problems with keeping on task. Calls unnecessary meetings. Could be a danger to the office."},
    {name: "Pamela Beasley", title: "Receptionist", email: "pbeasley@work.com",
    notes: "Exceptional worker. Answers the phones and is the Office Administrator."},
    {name: "Jim Halpert", title: "Salesman", email: "jhalpert@work.com",
    notes: "Meets sales quota. Not likely to apply himself for career. Sarcastic."},
    {name: "Toby Flenderson", title: "HR Rep", email: "tflenderson@work.com",
    notes: "Extremely boring, but does good work. Uneasy relationship with Michael Scott."},
    {name: "Dwight Schrute", title: "Salesman", email: "dschrute@work.com",
    notes: "Determined worker, intense good worker, hard worker, terrific. Bears, Beets, Battlestar Galactica."},
  ];

  var tbody=$('<tbody/>');

  $.each(employee,function(i,e){
    var tr = $('<tr></tr>').addClass('clickable-row');
      tr.append('<td><a href="/view" />'+e.name+'<td/>');
      tr.append('<td>'+e.title+'<td/>');
      tr.append('<td>'+e.email+'<td/>');
      tbody.append(tr);
  });

  $('table').append(tbody);

  $(document).ready(function() {
    $('#my-table tr').click(function() {
      var href = $(this).find("a").attr("href");
      if(href) {
        window.location = href;
      }
    });
  });
</script>
